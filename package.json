{
  "name": "angular-webpack-biolerplate",
  "version": "1.0.0",
  "description": "boilerplate for bootstraping angular app using webpack including tests (no css framework)",
  "main": "./server.js",
  "scripts": {
    "test": "karma start karma.conf.js --no-auto-watch --single-run",
    "start": "node ./bin/www",
    "build:production": "webpack -p --devtool --progress"
  },
  "author": "Alon Valadji",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.4.5",
    "angular-ui-router": "^0.2.15",
    "body-parser": "^1.13.3",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "jquery": "^2.1.4",
    "morgan": "^1.6.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.5",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.2",
    "karma": "^0.13.9",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon-chai": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.2",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "phantomjs": "^1.9.18",
    "raw-loader": "^0.5.1",
    "webpack": "^1.12.1",
    "webpack-dev-middleware": "^1.2.0"
  }
}
